<?xml version="1.0" encoding="utf-8"?>
<Application xmlns="http://xamarin.com/schemas/2014/forms" xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" x:Class="MIMobileApp.App">
    <Application.Resources>
        <ResourceDictionary>
            <Color x:Key="LoginBlue">#1A2241</Color>
            <Color x:Key="LoginWhite">#FFFFFF</Color>
            <Style TargetType="Label"
                   x:Key="SignUpLabel">
                <Setter Property="HorizontalTextAlignment" Value="Start"/>
            </Style>
        </ResourceDictionary>
    </Application.Resources>
</Application>